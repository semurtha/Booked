<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.semurtha.booked">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".EditReviewActivity"
            android:label="@string/edit_review_title"
            android:exported="false" />
        <activity
            android:name=".ReviewDetailActivity"
            android:label="@string/review_detail_title"
            android:exported="false" />
        <activity
            android:name=".NewReviewActivity"
            android:label="@string/add_review_title"
            android:exported="false" />
        <activity
            android:name=".UserFeedActivity"
            android:label="@string/user_feed_title"
            android:exported="false" />
        <activity
            android:name=".SignUpActivity"
            android:label="@string/sign_up_title"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:label="@string/sign_in_title"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>